{
  "name": "pm2-ui",
  "version": "1.0.0",
  "description": "pm2 ui and api - built with nuxt",
  "author": "jasenmichael",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
