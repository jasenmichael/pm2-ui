    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/font-awesome": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "detect-port": "^1.3.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "nuxt-fontawesome": "^0.4.0",
    "shelljs": "^0.8.3",
    "vue-json-pretty": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9",
    "@nuxtjs/eslint-config": "^1.0.1",
